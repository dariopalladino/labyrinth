# Labyrinth Configuration Example
# Copy this file to .env and modify the values as needed

# A2A SDK Configuration
LABYRINTH_A2A_BASE_URL=https://api.a2a.google.com
LABYRINTH_A2A_API_KEY=your_api_key_here
LABYRINTH_A2A_PROJECT_ID=your_project_id_here

# Agent Configuration
LABYRINTH_AGENT_NAME=my-agent
LABYRINTH_AGENT_DESCRIPTION=My Labyrinth agent
LABYRINTH_AGENT_PORT=8080
LABYRINTH_AGENT_HOST=localhost

# Communication Settings  
LABYRINTH_DEFAULT_TIMEOUT=30
LABYRINTH_RETRY_ATTEMPTS=3
LABYRINTH_RETRY_DELAY=1.0

# Logging Configuration
LABYRINTH_LOG_LEVEL=INFO
LABYRINTH_LOG_FORMAT=json

# Task Management
LABYRINTH_TASK_DEFAULT_TIMEOUT=300
LABYRINTH_TASK_CLEANUP_INTERVAL=3600
